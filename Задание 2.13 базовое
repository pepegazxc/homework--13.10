import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        System.out.print("Введите n трехзначное число:  ");
        int n = sc.nextInt();

        int a = n % 10;
        System.out.print(a);
        int b = n % 100;
        b = b / 10;
        System.out.print(b);
        int c = n / 100;
        System.out.print(c);


    }
}
